{
    "manifest_version": 3,
    "name": "591_map",
    "description": "Pin out the locations of 收藏物件 on google map",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {},
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                ""
            ]
        }
    ],
    "permissions": ["activeTab", "scripting"]
}